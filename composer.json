{
    "name": "origami/connect",
    "description": "OAuth connect",
    "keywords": ["oauth", "laravel"],
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0",
        "guzzlehttp/guzzle": "~4.0|~5.0",
        "league/oauth1-client": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Connect\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
